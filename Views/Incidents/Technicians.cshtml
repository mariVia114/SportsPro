@using SportsPro.Models.ManageViewModels
@model TechnicianIncidentViewModel
@{
    ViewData["Title"] = "Get Technician";
}

<form action="~/techincident/list" method="post">
    <h1>Technician</h1>
    <div asp-validation-summary="All" class="text-danger"></div>
    <div class="form-group" style="margin-bottom: 15px">
        @Html.DropDownListFor(model => model.TechnicianId, Model.Technicians, "Select a technician", new {@class = "form-control"} )
    </div>
    <div class="form-group">
        <input type="submit" class="btn btn-primary" value="Submit" />
    </div>
</form>
@section Scripts {
    @{
    await Html.RenderPartialAsync("_ValidationScriptsPartial");
}
}

